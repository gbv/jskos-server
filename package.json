    "jskos-validate": "^0.3.2",
    "lodash": "^4.17.15",
    "meow": "^7.0.1",
    "mongodb": "~3.5.7",
    "mongoose": "^5.9.13",
    "ndjson": "^1.5.0",
    "nocache": "^2.1.0",
    "passport": "^0.4.1",
    "passport-anonymous": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "portfinder": "^1.0.26",
    "typedi": "^0.8.0",
    "uuid": "^8.0.0"
  }
