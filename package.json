{
  "name": "jskos-server",
  "version": "1.1.7",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbv/jskos-server.git"
  },
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --exit --timeout 10000 --slow 2000",
    "start": "nodemon server.js",
    "lint": "./node_modules/.bin/eslint './**/*.js'",
    "fix": "./node_modules/.bin/eslint --fix './**/*.js'",
    "lint-staged": "lint-staged",
    "import": "./bin/import.js",
    "import-batch": "func() { tr '\\n' '\\0' < \"$2\"  | xargs -0 -n1 ./bin/import.js $1; }; func"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "pre-commit": "lint-staged",
  "bin": {
    "jskos-import": "./bin/import.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "ajv": "^6.12.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "eslint": "^7.0.0",
    "eslint-config-gbv": "github:gbv/eslint-config-gbv#ecaf810",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^10.2.2",
    "mocha": "^7.1.2",
    "mocha-eslint": "^6.0.0",
    "nodemon": "^2.0.3",
    "pre-commit": "^1.2.2",
    "request": "^2.88.2"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
